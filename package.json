{
  "name": "react-kitty",
  "version": "0.9.8",
  "description": "React Universally with PostCSS",
  "main": "lib/index.js",
  "jsnext:main": "lib/index.es.js",
  "module": "lib/index.es.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "bin": {
    "kitty-script": "./bin/kitty-script.js"
  },
  "scripts": {
    "start": "npm run rollup:build && webpack-dashboard -- node bin/kitty-script.js start",
    "just-build": "npm run rollup:build && node bin/kitty-script.js build",
    "build": "npm run just-build && node build/server/main.js",
    "clean": "rimraf build/client/* build/server/* lib/* bin/*",
    "nsp": "nsp check --output summary",
    "test": "cross-env NODE_PATH=src ava",
    "start:plain": "npm run rollup:build && node bin/kitty-script.js start",
    "test:watch": "cross-env ava --watch",
    "coverage": "nyc npm run test",
    "watch:test": "npm run test -- --watch",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ignore-path .eslintignore src",
    "rollup:build": "npm run clean && rollup --config rollup.script.cfg",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seedyee/react-kitty"
  },
  "keywords": [
    "react",
    "boilerplate",
    "universal",
    "javascript",
    "express",
    "webpack",
    "postcss",
    "cssmodules"
  ],
  "author": "seedyee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seedyee/react-kitty/issues"
  },
  "homepage": "https://github.com/seedyee/react-kitty#readme",
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "ava": {
    "require": [
      "./setup-test-env.js"
    ],
    "verbose": true,
    "babel": "inherit",
    "files": [
      "src/**/*.test.js"
    ]
  },
  "dependencies": {
    "classnames": "2.2.5",
    "immutable": "3.8.1",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
